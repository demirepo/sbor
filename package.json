{
  "name": "sbor-front",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "next dev",
    "build": "next build",
    "start": "next start",
    "lint": "next lint",
    "slint": "stylelint \"styles/**/*.{scss,css}\"",
    "slint:config": "stylelint . --print-config",
    "slint:fix": "stylelint \"styles/**/*.{scss,css}\" --fix",
    "send": "dotenv -e .env.local -- cross-var rsync -e 'ssh -i /.ssh/id_rsa -o StrictHostKeyChecking=no' --archive -zz --delete ./.next/ $SECRET_LOGIN@$SECRET_IP:$SECRET_PATH",
    "deploy": "npm run build && npm run send",
    "test": "jest --watch"
  },
  "dependencies": {
    "@hookform/resolvers": "^2.9.10",
    "@next/font": "13.0.7",
    "classnames": "^2.3.2",
    "dotenv-cli": "^6.0.0",
    "eslint": "^8.30.0",
    "eslint-config-next": "13.0.7",
    "lodash.isequal": "^4.5.0",
    "next": "13.0.7",
    "pg": "^8.6.5",
    "react": "18.2.0",
    "react-datepicker": "^4.8.0",
    "react-dom": "18.2.0",
    "react-hook-form": "^7.41.1",
    "swr": "^2.0.0",
    "typescript": "4.9.4",
    "zod": "^3.20.2"
  },
  "devDependencies": {
    "@swc/core": "^1.3.23",
    "@swc/jest": "^0.2.24",
    "@testing-library/jest-dom": "^5.16.5",
    "@testing-library/react": "^13.4.0",
    "@types/jest": "^29.2.4",
    "@types/next": "^9.0.0",
    "@types/node": "^18.11.16",
    "@types/pg": "^8.6.5",
    "@types/react": "18.0.26",
    "@types/react-datepicker": "^4.8.0",
    "@types/react-dom": "18.0.9",
    "cross-var": "^1.1.0",
    "dotenv": "^16.0.3",
    "jest": "^29.3.1",
    "jest-environment-jsdom": "^29.3.1",
    "prettier": "^2.8.1",
    "sass": "^1.57.0",
    "stylelint": "^14.16.0",
    "stylelint-config-prettier": "^9.0.4",
    "stylelint-config-standard": "^29.0.0",
    "stylelint-config-standard-scss": "^6.1.0",
    "stylelint-declaration-block-no-ignored-properties": "^2.6.0",
    "stylelint-order": "^5.0.0",
    "stylelint-scss": "^4.3.0"
  }
