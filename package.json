{
    "name": "sbor-front",
    "version": "0.1.0",
    "private": true,
    "scripts": {
        "dev": "next dev",
        "build": "next build",
        "start": "next start",
        "lint": "next lint",
        "slint": "stylelint \"styles/**/*.{scss,css}\"",
        "slint:config": "stylelint . --print-config",
        "slint:fix": "stylelint \"styles/**/*.{scss,css}\" --fix",
        "send": "dotenv -e .env -- cross-var rsync -e 'ssh -i /.ssh/id_rsa -o StrictHostKeyChecking=no' --archive -zz --delete ./.next/ $SECRET_LOGIN@$SECRET_IP:$SECRET_PATH",
        "deploy": "npm run build && npm run send"
    },
    "dependencies": {
        "@next/font": "13.0.7",
        "@types/node": "18.11.15",
        "@types/react": "18.0.26",
        "@types/react-dom": "18.0.9",
        "classnames": "^2.3.2",
        "eslint": "8.29.0",
        "eslint-config-next": "13.0.7",
        "next": "13.0.7",
        "pg": "^8.6.5",
        "react": "18.2.0",
        "react-datepicker": "^4.8.0",
        "react-dom": "18.2.0",
        "swr": "^2.0.0",
        "typescript": "4.9.4"
    },
    "devDependencies": {
        "@types/pg": "^8.6.5",
        "@types/react-datepicker": "^4.8.0",
        "dotenv": "^16.0.3",
        "prettier": "^2.8.1",
        "sass": "^1.56.2",
        "stylelint": "^14.16.0",
        "stylelint-config-prettier": "^9.0.4",
        "stylelint-config-standard": "^29.0.0",
        "stylelint-config-standard-scss": "^6.1.0",
        "stylelint-declaration-block-no-ignored-properties": "^2.6.0",
        "stylelint-order": "^5.0.0",
        "stylelint-scss": "^4.3.0"
    }
}
